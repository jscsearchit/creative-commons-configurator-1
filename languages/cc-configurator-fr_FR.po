msgid "is licensed under a"
msgstr "est publié sous licence"

#: /home/jani/creative-commons-configurator/cc-configurator.php:670
msgid ""
"Terms and conditions beyond the scope of this license may be available at"
msgstr ""
"Les termes et conditions au-delà de cette licence sont disponible ici : "

#: /home/jani/creative-commons-configurator/cc-configurator.php:695
msgid "Blog Name"
msgstr "Nom du blog"

#: /home/jani/creative-commons-configurator/cc-configurator.php:696
msgid "First + Last Name"
msgstr "Prénom + Nom"

#: /home/jani/creative-commons-configurator/cc-configurator.php:697
msgid "Last + First Name"
msgstr "Nom + Prénom"

#: /home/jani/creative-commons-configurator/cc-configurator.php:698
msgid "Nickname"
msgstr "Surnom"

#: /home/jani/creative-commons-configurator/cc-configurator.php:699
msgid "Displayed Name"
msgstr "Nom affiché"
